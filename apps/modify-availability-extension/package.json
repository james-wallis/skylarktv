{
  "name": "modify-availability-extension",
  "private": true,
  "version": "0.0.1",
  "description": "Chrome extension that enables overwriting Skylark Availability Dimensions on any website powered by Skylark",
  "type": "module",
  "repository": {
    "url": "https://github.com/skylark-platform/reference-apps"
  },
  "scripts": {
    "tsc": "tsc",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@skylark-reference-apps/dimensions-drawer": "*"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.17",
    "@types/chrome": "^0.0.237",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
